er>Lines 1</header>\n        <input type=\"range\" inline title=\"Line Active 1\" name=\"line1Active1\" min=\"0\" max=\"100\" step=\"1\" value=\"90\">\n        <input type=\"range\" inline title=\"Line Active 2\" name=\"line1Active2\" min=\"0\" max=\"100\" step=\"1\" value=\"70\" condition=\"cardsAmount1 >1\">\n        <input type=\"range\" inline title=\"Line Active 3\" name=\"line1Active3\" min=\"0\" max=\"100\" step=\"1\" value=\"85\" condition=\"cardsAmount1 >2\">\n        <input type=\"range\" inline title=\"Line Active 4\" name=\"line1Active4\" min=\"0\" max=\"100\" step=\"1\" value=\"88\" condition=\"cardsAmount1 >3\">\n        <input type=\"range\" inline title=\"Line Active 5\" name=\"line1Active5\" min=\"0\" max=\"100\" step=\"1\" value=\"88\" condition=\"cardsAmount1 >4\">\n        <header condition=\"listCount >1\">Lines 2</header>\n        <input type=\"range\" inline title=\"Line Active 1\" name=\"line2Active1\" min=\"0\" max=\"100\" step=\"1\" value=\"94\" condition=\"listCount >1\">\n        <input type=\"range\" inline title=\"Line Active 2\" name=\"line2Active2\" min=\"0\" max=\"100\" step=\"1\" value=\"88\" condition=\"cardsAmount2 >1 &amp;&amp; listCount >1\">\n        <input type=\"range\" inline title=\"Line Active 3\" name=\"line2Active3\" min=\"0\" max=\"100\" step=\"1\" value=\"80\" condition=\"cardsAmount2 >2 &amp;&amp; listCount >1\">\n        <input type=\"range\" inline title=\"Line Active 4\" name=\"line2Active4\" min=\"0\" max=\"100\" step=\"1\" value=\"90\" condition=\"cardsAmount2 >3 &amp;&amp; listCount >1\">\n        <input type=\"range\" inline title=\"Line Active 5\" name=\"line2Active5\" min=\"0\" max=\"100\" step=\"1\" value=\"93\" condition=\"cardsAmount2 >4 &amp;&amp; listCount >1\">\n        <header>Lines Style</header>\n        <input type=\"color\" title=\"Lines Text Color\" name=\"linesTextColor\" value=\"#C4CFDE\">\n        <input type=\"color\" title=\"Lines BG\" name=\"linesBg\" value=\"#212428\">\n        <input type=